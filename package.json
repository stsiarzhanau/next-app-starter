    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.1",
    "@vitest/ui": "^1.1.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.56.0",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
